[[nx, E(n)], H(b)]
[[nx, H(b)], E(n)] + [[H(b), E(n)], nx]
[nx_h, E(n)] + [[H(b), E(n)], nx]
[nx_h, E(n)] + [C(b, n) * E(n), nx]
