[x, [y, z]] + [y, [z, x]] + [z, [x, y]] = 0
[[y, z], x] + [[z, x], y] + [[x, y], z] = 0
[[a, b], c] + [[b, c], a] + [[c, a], b] = 0
[[a, b], c] =-[[b, c], a] - [[c, a], b]
[[a, b], c] = [[c, b], a] + [[a, c], b]

