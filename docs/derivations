[[nx, E(n)], F(b)]
[[nx, F(b)], E(n)] + [[H(b), F(n)], nx]
[nx_f, E(n)] + [[F(b), E(n)], nx]
if n == b {
    [nx_f, E(n)] - [nx, H(b)] 
} else {
    [nx_f, E(n)]
}
if n == b {
    [nx_f, E(n)] - nx_h
} else {
    [nx_f, E(n)]
